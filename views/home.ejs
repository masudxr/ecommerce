                    <p>Uploaded Time:<%=new Date(products[i].Date).getDate() %>/<%= new
                                Date(products[i].Date).getMonth() %>/<%= new Date(products[i].Date).getFullYear()%>
                    </p>
                    <p>Seller ID:<%= products[i].User_id %>
                    </p>
                    <br>
                    <% if (logedIn==products[i].User_id) {%>
                        <a href=/<%=products[i].p_id %>/remove ><button>Delete Item</button></a>
                        <% } %>
                </div>
            <% } %>
        <% } %>

